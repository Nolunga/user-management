    "date-fns": "2.29.3",
    "framer-motion": "^10.12.10",
    "lodash": "4.17.21",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.39.5",
    "react-icons": "4.6.0",
    "react-router-dom": "6.4.3"
